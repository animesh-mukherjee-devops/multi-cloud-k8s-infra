# Environment Configuration Example
ENVIRONMENT=dev
CLOUD_PROVIDER=digitalocean

# DigitalOcean Configuration
DIGITALOCEAN_TOKEN=your_do_token_here
DO_SPACES_KEY=your_spaces_key_here
DO_SPACES_SECRET=your_spaces_secret_here
DO_REGION=nyc3
DO_NODE_SIZE=s-2vcpu-2gb
DO_NODE_COUNT=2

# Azure Configuration
ARM_CLIENT_ID=your_client_id_here
ARM_CLIENT_SECRET=your_client_secret_here
ARM_SUBSCRIPTION_ID=your_subscription_id_here
ARM_TENANT_ID=your_tenant_id_here
AZURE_LOCATION=East US
AZURE_NODE_SIZE=Standard_D2s_v3
AZURE_NODE_COUNT=2